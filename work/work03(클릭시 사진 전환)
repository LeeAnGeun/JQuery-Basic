<title>Insert title here</title>

<script src="https://code.jquery.com/jquery-3.5.1.min.js"></script>

</head>
<body>

<h1>room image</h1>
<img id="img1" alt="" src="./image/photo_a.jpg">
<img id="img2" alt="" src="./image/photo_b.jpg"> 

<script type="text/javascript">

$(document).ready(function() {
	// 웹페이지를 열었을 경우
	$("#img1").show();
    $("#img2").hide();
    
    /*img1을 클릭했을 때 img2를 보여줌*/
    $("#img1").click(function(){
        $("#img1").hide();
        $("#img2").show();
    });

    /*img2를 클릭했을 때 img1을 보여줌*/
    $("#img2").click(function(){
        $("#img1").show();
        $("#img2").hide();
    });
});


</script>
</body>
</html>
