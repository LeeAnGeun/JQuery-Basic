<select id="food">
	<option>햄버거</option>
	<option>피자</option>
	<option>치킨</option>
	
</select>

<button type="button" id="append">추가(뒤)</button>
<button type="button" id="prepend">추가(앞)</button>

<script type="text/javascript">
$(function () {
	$("#append").click(function() {
		
	//	$("p").append("<br>append 추가 요소입니다");
	
	//	$("ol").append( "<li>포도</li> ");
	
		$("#food").append( "<option>핫바</option>");
	});
	
	$("#prepend").click(function() {
		
	//	$("p").prepend("prepend 추가 요소입니다.<br>")
	
	//	$("ol").prepend( "<li>딸기</li>");
	
		$("#food").prepend( "<option>핫바</option>");
	});
});

</script>

